{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Configuration for repositories marked 'security-updates-only' in the InteractionDesignFoundation organisation",
  "extends": [
    "local>InteractionDesignFoundation/.github:renovate-config",
    ":maintainLockFilesDisabled"
  ],
  "packageRules": [
    {"matchPackagePatterns": ["*"], "enabled": false},
    {"matchPackageNames": ["php"], "enabled": true}
  ]
}
